@book{zacklad_reseaux_2007,
  title = {{R\'eseaux et communaut\'es d'imaginaire docum\'ediatis\'ees}},
  author = {Zacklad, Manuel},
  year = {2007},
  month = apr,
  publisher = {{Peter Lang}},
  urldate = {2022-03-10},
  abstract = {Dans le renouveau des travaux actuels sur le document (cf. P\'edauque 2003) l'accent est souvent mis sur leurs usages professionnels dans le contexte de l'entreprise, de la recherche scientifique, des institutions artistiques ou culturelles, par exemple. Dans ce texte, nous souhaitons mettre plus particuli\`erement l'accent sur l'importance des documents dans le contexte des usages \guillemotleft{} grand public \guillemotright, de leurs r\^oles tr\`es divers aupr\`es de populations de tous \^ages dans le cadre d'activit\'es qui sont souvent qualifi\'ees d'activit\'es de loisir. Nous verrons que, dans ce cadre \'egalement, la num\'erisation transforme profond\'ement les pratiques et que les th\'eorisations actuelles du document et notamment notre approche en termes de processus de documentarisation, permet de les appr\'ehender de mani\`ere originale. En particulier, nous d\'efendrons l'id\'ee selon laquelle ces nouveaux usages sont caract\'eristiques d'une nouvelle forme de socialit\'e que P\'edauque (2006) qualifie de \guillemotleft{} post-moderne \guillemotright{} et qui correspond pour nous \`a l'expression de \guillemotleft{} modernit\'e sans fronti\`ere \guillemotright, pour reprendre le titre de l'ouvrage de l'anthropologue indo-am\'ericain Arjun Appadurai \guillemotleft{} Modernity at Large, Cultural dimension of globalization \guillemotright{} (Appardurai 1996).},
  isbn = {978-3-631-56294-9},
  langid = {french}
}

