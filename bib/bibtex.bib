@article{larrue_intermedialite_2020,
  title = {{De l'interm{\'e}dialit{\'e} {\`a} l'excommunication}},
  author = {Larrue, Jean-Marc},
  year = {2020},
  month = nov,
  journal = {Cahiers d'{\'E}tudes Germaniques},
