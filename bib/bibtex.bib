@article{vitali-rosati_fait_2021,
  title = {{Le fait num\'erique comme \guillemotleft ~conjonctures m\'ediatrices~\guillemotright}},
  author = {{Vitali-Rosati}, Marcello},
  year = {2021},
  journal = {Communication \& langages},
  volume = {208--209},
  number = {2-3},
  pages = {155--170},
  publisher = {{Presses Universitaires de France}},
  address = {{Paris cedex 14}},
  issn = {0336-1500},
  doi = {10.3917/comla1.208.0155},
  urldate = {2023-10-27},
  abstract = {De plus en plus, l'interm\'edialit\'e est mobilis\'ee en Sciences de l'information et de la communication, souvent comme un concept pour d\'ecrire des ph\'enom\`enes d'hybridations entre diff\'erents m\'edias, moins fr\'equemment comme une approche ou une qualit\'e du regard sur les objets de recherche. Telle qu'elle est internationalement d\'eploy\'ee, la notion est pourtant porteuse de cette capacit\'e op\'eratoire comme pratique de recherche. Pour la premi\`ere fois en France, Communication \& langages consacre un num\'ero double \`a cette approche pens\'ee et mise en oeuvre depuis 20 ans au sein du Centre de Recherches Interm\'ediales sur les Arts, les Lettres et les Techniques (CRIalt) par les membres de l'\guillemotleft ~\'Ecole de Montr\'eal~\guillemotright. Moins connue en Europe que l'approche allemande, celle-ci est pourtant particuli\`erement int\'eressante pour les SIC, \`a la fois parce qu'elle mobilise et th\'eorise des notions qui y sont centrales \textendash{} les m\'edias, les m\'ediations \textendash{} ainsi que des notions corollaires qui nous sont moins famili\`eres \textendash{} la m\'edialit\'e, par exemple \textendash{} et parce que les perspectives qui sont les siennes nous invitent parfois \`a nous d\'ecentrer pour questionner les n\^otres. Au coeur de cette d\'emarche interm\'ediale r\'eside la probl\'ematique de la relation, qui touche directement aux processus de communication que nous analysons. Elle invite en effet \`a revendiquer une pens\'ee complexe, consid\'erant sans sch\'ematisme la pluralit\'e des plans qui sont constitutifs de tout type de ph\'enom\`ene social, et en examinant avec une attention particuli\`ere les liens, les noeuds, les tensions, les dynamiques en jeu et leurs r\'e\'equilibrages permanents. Prenant pour sujet des objets, signes et pratiques tr\`es divers, qui illustrent la richesse et la cr\'eativit\'e de ses mises en oeuvre au CRIalt, le pr\'esent dossier t\'emoigne d'une interdisciplinarit\'e active qui a tout le potentiel pour nourrir nos propres pratiques de recherches en Lettres et Sciences humaines et sociales.},
  langid = {french},
  keywords = {conjonctures m\'ediatrices,m\'ediation,m\'etaontologie,ontologie}
}

