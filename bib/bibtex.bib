@article{bernardot_plongee_2018,
  title = {{Plong\'ee dans les m\'etaphores et repr\'esentations liquides de la soci\'et\'e num\'erique}},
  author = {Bernardot, Marc},
  year = {2018},
  month = dec,
  journal = {Netcom. R\'eseaux, communication et territoires},
  number = {32-1/2},
  pages = {29--60},
  publisher = {{Netcom Association}},
  issn = {0987-6014},
  doi = {10.4000/netcom.2886},
  urldate = {2022-04-20},
  abstract = {Le jargon num\'erique utilise beaucoup de mots liquides. Qu'est-ce que ces m\'etaphores liquides fondent et fa\c{c}onnent ? Que montrent-elles et cachent-elles ? Sont-elles utilis\'ees comme une m\'etaphore vivante, ou d\'egrad\'ee, nomade ou \guillemotleft{} spongieuse \guillemotright{} ? Cet article est divis\'e en trois parties pour essayer de r\'epondre \`a ces questions. La premi\`ere partie d\'ecrit divers emplois du champ s\'emantique \guillemotleft{} liquide \guillemotright{} tel qu'il est mobilis\'e \`a la fois dans les lexiques techniques, dans l'imaginaire collectif et dans les cadres cognitifs des pionniers de l'Internet et des acteurs actuels (1). Dans une seconde partie, l'analyse se penche sur la mani\`ere dont les r\'ecits n\'egatifs et cataclysmiques de la soci\'et\'e num\'erique sont discursivement fond\'es sur des repr\'esentations \guillemotleft{} liquides \guillemotright{} de l'opacit\'e, de fuites, d'inondations d'information incontr\^ol\'ees et de risques de d\'etournement mon\'etaire (2). La troisi\`eme partie examine quelques perspectives utopiques et positives des technologies de r\'eseau lorsqu'elles appliquent le champ s\'emantique de la liquidit\'e pour d\'ecrire les progr\`es de la transparence, de la fluidit\'e, de la mobilit\'e due \`a l'\'economie num\'erique (3.). La conclusion explore les perspectives th\'eoriques et politiques propuls\'ees par les m\'etaphores liquides ainsi que les enjeux de ces lectures interpr\'etatives ambivalentes du futur de l'\`ere num\'erique, pour faire le point sur la tension entre la peur de la surveillance globale \guillemotleft{} liquide \guillemotright{} et l'espoir de la \guillemotleft{} liquidation \guillemotright{} des syst\`emes de domination.},
  copyright = {Netcom \textendash\hspace{0.6em}R\'eseaux, communication et territoires est mis \`a disposition selon les termes de la licence Creative Commons Attribution - Pas d'Utilisation Commerciale - Pas de Modification 4.0 International.},
  langid = {french},
  keywords = {architexte,champ s\'emantique,globalisation,liquidation,liquide,liquidit\'e,m\'etaphore,num\'erique,repr\'esentation,sociologie}
}

